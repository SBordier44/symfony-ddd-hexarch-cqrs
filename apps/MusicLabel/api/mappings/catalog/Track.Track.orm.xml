<?xml version="1.0" encoding="utf-8"?>
<!--suppress XmlDefaultAttributeValue -->
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Masfernandez\MusicLabel\Backoffice\Catalog\Domain\Track\Track"
            repository-class="Masfernandez\MusicLabel\Backoffice\Catalog\Domain\Track\TrackRepository"
            table="track">
        <id name="id" type="TrackId" column="id"/>
        <field name="name" type="TrackName" column="name" length="255" nullable="false"/>
        <field name="price" type="TrackPrice" column="price" nullable="false"/>
    </entity>
</doctrine-mapping>
