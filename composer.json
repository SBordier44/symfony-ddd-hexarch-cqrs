{
    "name": "masfernandez/symfony-ddd-hexarch-cqrs",
    "description": "cool description here",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Ángel Sánchez Fernández",
            "email": "mangel.sanfer@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Masfernandez\\MusicLabel\\": "src/",
            "Masfernandez\\MusicLabel\\Infrastructure\\Api\\": "apps/MusicLabel/api/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masfernandez\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:rkeet/behatch-contexts.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo_mysql": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "lambdish/phunctional": "^2.1",
        "lcobucci/jwt": "^4.1",
        "nelmio/cors-bundle": "^2.2",
        "ramsey/uuid-doctrine": "^1.8",
        "sensio/framework-extra-bundle": "^6.2",
        "snc/redis-bundle": "^3.6",
        "symfony/amqp-messenger": "6.1.*",
        "symfony/console": "6.1.*",
        "symfony/doctrine-messenger": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/error-handler": "6.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "6.1.*",
        "symfony/messenger": "6.1.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/runtime": "6.1.*",
        "symfony/uid": "6.1.*",
        "symfony/validator": "6.1.*",
        "symfony/yaml": "6.1.*"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "behatch/contexts": "dev-php80 as 3.3.0",
        "brianium/paratest": "^6.3",
        "fakerphp/faker": "^1.19",
        "friends-of-behat/mink": "^1.10",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-extension": "^2.6",
        "friends-of-behat/symfony-extension": "2.2.x-dev",
        "masfernandez/nginx-proxy-local-development": "dev-master",
        "mockery/mockery": "^1.5",
        "php-coveralls/php-coveralls": "^2.5",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/stopwatch": "^5.4",
        "vimeo/psalm": "^4.20"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        },
        "bin-dir": "apps/MusicLabel/api/bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
